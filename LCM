#include<stdio.h>
main()
{
	int num1 ,num2;
	printf("Enter first number :\t");
	scanf("%d",&num1);
	printf("Enter second number :\t");
	scanf("%d",&num2);
	printf("The LCM of and is: %d",LCM(num1,num2));
}
int lcm (int a, int b)
{
	static int m=0;
	m=m+b;
	if(m%a==0 && m%b==0)
	{
		return m;
	}
	return lcm(a,b);
}
